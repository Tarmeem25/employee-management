<html xmlns:t="http://tapestry.apache.org/schema/tapestry_5_4.xsd">
<head>
    <title>Employee List</title>
</head>
<body>
<h1>Employee List</h1>
<table>
    <tr>
        <th>Id</th>
        <th>Name</th>
        <th>Age</th>
        <th>Address</th>
    </tr>
    <t:loop source ="employees" value= "employee" >
        <tr>
            <td>${employee.id}</td>
            <td>${employee.name}</td>
        <td>${employee.age}</td>
        <td>${employee.address}</td>
    </tr>
    </t:loop>
</table>
</body>
</html>
